@techreport{feng_how_2017,
	address = {Rochester, NY},
	type = {{SSRN} {Scholarly} {Paper}},
	title = {How {Research} in {Production} and {Operations} {Management} {May} {Evolve} in the {Era} of {Big} {Data}},
