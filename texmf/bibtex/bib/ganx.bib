@techreport{ban_big_2018,
	address = {Rochester, NY},
	type = {{Forthcoming in Operations Research. SSRN} {Scholarly} {Paper}},
	title = {The {Big} {Data} {Newsvendor}: {Practical} {Insights} from {Machine} {Learning}},
	shorttitle = {The {Big} {Data} {Newsvendor}},
	url = {https://papers.ssrn.com/abstract=2559116},
	abstract = {We investigate the data-driven newsvendor problem when one has n observations of p features related to the demand as well as historical demand data. Rather than a two-step process of first estimating a demand distribution then optimizing for the optimal order quantity, we propose solving the "Big Data" newsvendor problem via single step machine learning algorithms. Specifically, we propose algorithms based on the Empirical Risk Minimization (ERM) principle, with and without regularization, and an algorithm based on Kernel-weights Optimization (KO). The ERM approaches, equivalent to high-dimensional quantile regression, can be solved by convex optimization problems and the KO approach by a sorting algorithm. We analytically justify the use of features by showing that their omission yields inconsistent decisions. We then derive finite-sample performance bounds on the out-of-sample costs of the feature-based algorithms, which quantify the effects of dimensionality and cost parameters. Our bounds, based on algorithmic stability theory, generalize known analyses for the newsvendor problem without feature information. Finally, we apply the feature-based algorithms for nurse staffing in a hospital emergency room using a data set from a large UK teaching hospital and find that (i) the best ERM and KO algorithms beat the best practice benchmark by 23\% and 24\% respectively in the out-of-sample cost, and (ii) the best KO algorithm is faster than the best ERM algorithm by three orders of magnitude and the best practice benchmark by two orders of magnitude.},
	language = {en},
	number = {ID 2559116},
	urldate = {2018-04-11},
	institution = {Social Science Research Network},
	author = {Ban, Gah-Yi and Rudin, Cynthia},
	month = mar,
	year = {2018},
	keywords = {ref, big data, optStat, newsvendor, machine learning, quantile regression, Sample Average Approximation, statistical learning theory},
	file = {Ban_Rudin_2018_The Big Data Newsvendor - Practical Insights from Machine Learning.pdf:/home/a/Zotero/storage/YKMFQM7K/Ban_Rudin_2018_The Big Data Newsvendor - Practical Insights from Machine Learning.pdf:application/pdf;Snapshot:/home/a/Zotero/storage/ZYV9QYV8/papers.html:text/html}
}

@article{chen_optimal_2011,
	title = {Optimal {Selling} {Scheme} for {Heterogeneous} {Consumers} with {Uncertain} {Valuations}},
	volume = {36},
	issn = {0364-765X},
	url = {https://pubsonline.informs.org/doi/abs/10.1287/moor.1110.0514},
	doi = {10.1287/moor.1110.0514},
	abstract = {In this paper, we investigate the optimal selling scheme for a capacity-constrained seller who faces both aggregate demand and individual valuation uncertainties. As each consumer privately observes an initial estimate prior to the transactions and an updated information postpurchase, the problem exhibits the sequential screening feature and return policy emerges as part of the optimal selling scheme. We show that the optimal selling scheme nicely resembles the classical multiunit auction design, and the product is returned too often compared to the efficient scenario. Furthermore, even though the postpurchase information update is privately observed by the individual consumers, the seller does not give away additional surplus to the consumers. Our general framework can accommodate various heterogeneities and uncertainties regarding hassle costs, shipping costs, salvage values, and reservation values. Our results go through even if the seller cannot commit to the refund amounts, the seller has flexible capacity, or consumers have multiunit demands.},
	number = {4},
	urldate = {2018-04-19},
